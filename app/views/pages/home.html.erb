<div class="container">
    <div class="jumbotron text-center">
        <h1 class="">Rails Blog Engine</h1>
        <% if session[:user_id] %>
            <%= link_to 'Log out', logout_path, class: 'btn btn-primary btn-lg', method: 'delete' %>
        <% else %>
            <%= link_to 'Signup', signup_path, class: 'btn btn-primary btn-lg' %>
            <%= link_to 'Log in', login_path, class: 'btn btn-primary btn-lg' %>
        <% end %>
    </div>
</div>