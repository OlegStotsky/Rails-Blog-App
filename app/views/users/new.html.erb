<div class="container">
    <div class="row">
        <div class="col-xs-12 col-md-6 col-md-offset-3">
            <% if @user.errors.any? %>
                <%= render 'shared/errors', target: @user %>
            <% end %>
            <%= render 'form', user: @user, form_name: 'Create New User', button_text: 'Sign Up' %>
        </div>
    </div>
</div>