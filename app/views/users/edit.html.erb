<div class="container">
    <div class="row">
        <% if @user.errors.any? %>
            <%= render 'shared/errors', target: @user %>
        <% end %>
        <div class="col-xs-12 col-md-6 col-md-offset-3">
            <%= render 'form', user: @user, form_name: 'Edit User', button_text: 'Update Account' %>
        </div>
    </div>
</div>